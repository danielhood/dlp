                      00069 ; ------------------------------------------------------------------------------
                      00070 ; Macro to split the 7 bits across the high and low PWM output registers
                      00071 ; by putting them into the TMP_CV_BYTE_L and TMP_CV_BYTE_H
                      00072 ;       The 7th bit should end up in the TMP_CV_BYTE_H
                      00073 ;       The remaining 6 bits should be in the upper 6 bits of TMP_CV_BYTE_L
                      00074 ;       Upon return:
                      00075 ;               TMP_CV_BYTE_H should be placed into PDCxH: '--00000d'
                      00076 ;               TMP_CV_BYTE_L should be placed into PDCxL: 'dddddd00'
                      00077 ;
                      00078 _SPLIT_CV_VALUE macro
                      00079         movwf   TMP_CV_BYTE_L   ; W should contain the 7bit CV value (0..127)
                      00080         bcf     STATUS,C        ; Ensure C = 0
                      00081         rlncf   TMP_CV_BYTE_L,F
                      00082         rlcf    TMP_CV_BYTE_L,F ; pushes the msbit of 7bit value into C
                      00083         clrf    TMP_CV_BYTE_H
                      00084         rlcf    TMP_CV_BYTE_H   ; rolls on the C bit 
