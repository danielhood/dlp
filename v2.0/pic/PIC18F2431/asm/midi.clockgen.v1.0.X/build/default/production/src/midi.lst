                      00077 ; ------------------------------------------------------------------------------
                      00078 ; Macro to split the 7 bits across the high and low PWM output registers
                      00079 ; by putting them into the TMP_CV_BYTE_L and TMP_CV_BYTE_H
                      00080 ;       The 7th bit should end up in the TMP_CV_BYTE_H
                      00081 ;       The remaining 6 bits should be in the upper 6 bits of TMP_CV_BYTE_L
                      00082 ;       Upon return:
                      00083 ;               TMP_CV_BYTE_H should be placed into PDCxH: '--00000d'
                      00084 ;               TMP_CV_BYTE_L should be placed into PDCxL: 'dddddd00'
                      00085 ;
                      00086 _SPLIT_CV_VALUE macro
                      00087         movwf   TMP_CV_BYTE_L   ; W should contain the 7bit CV value (0..127)
                      00088         bcf     STATUS,C        ; Ensure C = 0
                      00089         rlncf   TMP_CV_BYTE_L,F
                      00090         rlcf    TMP_CV_BYTE_L,F ; pushes the msbit of 7bit value into C
                      00091         clrf    TMP_CV_BYTE_H
                      00092         rlcf    TMP_CV_BYTE_H   ; rolls on the C bit
                      00093         endm
                      00094 
                      00095 _SPLIT_CV_VALUE2        macro
                      00096         ;rlncf  WREG            ; Double the value since we're using a period of 256 instead of 128
                      00097         movwf   TMP_CV_BYTE_L   ; W should contain the 8bit CV value
                      00098         andlw   0xC0
                      00099         movwf   TMP_CV_BYTE_H   ;
                      00100         rlncf   TMP_CV_BYTE_L,F
                      00101         rlncf   TMP_CV_BYTE_L,F ; pushes the msbit of 7bit value into C
                      00102         swapf   TMP_CV_BYTE_H,F
                      00103         rrncf   TMP_CV_BYTE_H,F
                      00104         rrncf   TMP_CV_BYTE_H,F
                      00105         endm
